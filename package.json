{
  "name": "interactiveflow",
  "version": "1.0.0",
  "description": "Streamline your Discord UI with declarative Containers & Stateful Pagination.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "discord",
    "discord.js",
    "components",
    "containers",
    "pagination",
    "ui",
    "builder"
  ],
  "author": "rly-dev",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rly-dev/InteractiveFlow"
  },
  "peerDependencies": {
    "discord.js": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "discord.js": "^14.18.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
  }
}
